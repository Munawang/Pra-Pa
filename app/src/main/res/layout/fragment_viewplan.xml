<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--phase-->
    <TextView
        android:id="@+id/textView4"
        android:layout_width="97dp"
        android:layout_height="61dp"
        android:text="@string/topic_phase"
        app:autoSizeTextType="uniform"
        app:layout_editor_absoluteX="16dp"
        app:layout_editor_absoluteY="64dp" />
    <EditText
        android:id="@+id/editText"
        android:layout_width="88dp"
        android:layout_height="58dp"
        android:ems="10"
        android:hint="@string/topic_phase"
        android:inputType="textPersonName"
        app:layout_editor_absoluteX="113dp"
        app:layout_editor_absoluteY="64dp" />

    <!--floor-->
    <TextView
        android:id="@+id/textView5"
        android:layout_width="81dp"
        android:layout_height="58dp"
        android:text="@string/topic_floor"
        app:autoSizeTextType="uniform"
        app:layout_editor_absoluteX="201dp"
        app:layout_editor_absoluteY="64dp" />
    <EditText
        android:id="@+id/editText2"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:ems="10"
        android:hint="@string/topic_floor"
        android:inputType="textPersonName"
        app:layout_editor_absoluteX="283dp"
        app:layout_editor_absoluteY="64dp" />

    <!--top room-->
    <ImageView
        android:id="@+id/top_1_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room"
        app:layout_editor_absoluteX="6dp"
        app:layout_editor_absoluteY="197dp" />
    <ImageView
        android:id="@+id/top_2_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_right_up"
        app:layout_editor_absoluteX="67dp"
        app:layout_editor_absoluteY="197dp" />
    <ImageView
        android:id="@+id/top_3_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room"
        app:layout_editor_absoluteX="128dp"
        app:layout_editor_absoluteY="197dp" />
    <ImageView
        android:id="@+id/top_4_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_right_up"
        app:layout_editor_absoluteX="189dp"
        app:layout_editor_absoluteY="197dp" />
    <ImageView
        android:id="@+id/top_5_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room"
        app:layout_editor_absoluteX="250dp"
        app:layout_editor_absoluteY="197dp" />
    <ImageView
        android:id="@+id/top_6_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_right_up"
        app:layout_editor_absoluteX="311dp"
        app:layout_editor_absoluteY="197dp" />

    <!--bottom room-->
    <ImageView
        android:id="@+id/bottom_1_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_left_down"
        app:layout_editor_absoluteX="6dp"
        app:layout_editor_absoluteY="326dp" />
    <ImageView
        android:id="@+id/bottom_2_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_right_down"
        app:layout_editor_absoluteX="67dp"
        app:layout_editor_absoluteY="326dp" />
    <ImageView
        android:id="@+id/bottom_3_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_left_down"
        app:layout_editor_absoluteX="128dp"
        app:layout_editor_absoluteY="326dp" />
    <ImageView
        android:id="@+id/bottom_4_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_right_down"
        app:layout_editor_absoluteX="189dp"
        app:layout_editor_absoluteY="326dp" />
    <ImageView
        android:id="@+id/bottom_5_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_left_down"
        app:layout_editor_absoluteX="250dp"
        app:layout_editor_absoluteY="326dp" />
    <ImageView
        android:id="@+id/bottom_6_view_plan"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:src="@drawable/room_right_down"
        app:layout_editor_absoluteX="311dp"
        app:layout_editor_absoluteY="326dp" />

</android.support.constraint.ConstraintLayout>